<h1>Login successful</h1>

<%= image_tag(current_user.image) %>

<table class="table">
  <tbody>
    <tr>
      <th style="width: 120px">UID</th>
      <td><%= current_user.uid %></td>
    </tr>
    <tr>
      <th>Provider</th>
      <td><%= current_user.provider %></td>
    </tr>
    <tr>
      <th>Email</th>
      <td><%= current_user.email %></td>
    </tr>
    <tr>
      <th>Display</th>
      <td><%= current_user.display_name %></td>
    </tr>
  </tbody>
</table>

